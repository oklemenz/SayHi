{
  "name": "sayhi",
  "version": "1.0.0",
  "description": "SAY Hi!",
  "main": "index.js",
  "scripts": {
    "pginit": "npm run pgdata && npm run pgstart && npm run pguser && npm run pgdb && npm run pgconnect",
    "pgdata": "(initdb -D pgdata || true)",
    "pgdb": "(createdb sayhi || true)",
    "pguser": "(createuser -P postgres || true)",
    "pgconnect": "(node db-connect || true)",
    "pgstart": "(pg_ctl -w -D pgdata start || true)",
    "pgstop": "(pg_ctl -w -D pgdata stop || true)",
    "pgsql": "psql --username postgres sayhi",
    "mongostart": "mongod",
    "start": "node index",
    "data:d": "npm run data:l && npm run data:i",
    "data:c": "node data/content",
    "data:k": "node data/check",
    "data:l": "node data/load",
    "data:i": "node data/icon",
    "data:a": "node data/activate",
    "data:v": "node data/validate",
    "data:f": "node data/favorite",
    "data:t": "node data/top",
    "data:p": "node data/primary",
    "match:c": "node match/cleanup",
    "ana:s": "node analytics/stage",
    "ana:a": "node analytics/activate",
    "ana:c": "node analytics/clear",
    "score:s": "node score/stage",
    "score:a": "node score/activate",
    "score:c": "node score/clear"
  },
  "author": "Oliver Klemenz",
  "license": "MIT",
  "dependencies": {
    "diacritics": "1.3.0",
    "firebase": "3.6.1",
    "firebase-admin": "4.0.3",
    "firebase-tools": "3.2.0",
    "lodash": "4.17.4",
    "pg": "6.1.0",
    "underscore": "1.8.3"
  }
}
