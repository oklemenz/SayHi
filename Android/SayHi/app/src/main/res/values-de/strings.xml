    <string name="InviteSubject">Einladung zu SAY Hi!</string>
    <string name="InviteBody">
        <![CDATA[
            <p>Hi!</p>
            <p>
            Ich nutze die App SAY Hi! und finde sie echt klasse!
            <br>
            Wenn du sie dir auch holst, dann können wir sie gemeinsam nutzen.
            </p>
            <p>
            Du kannst die App hier herunterladen:
            </p>
            <p>
            iOS:
            <br>
            <a href=\'%1$s\'>%2$s</a>
            <p>
            <p>
            Android:
            <br>
            <a href=\'%3$s\'>%4$s</a>
            </p>
            Bis bald!
            </p>
            <p>%5$s</p>
        ]]>
    </string>
