    <string name="InviteSubject">Invite to SAY Hi!</string>
    <string name="InviteBody">
        <![CDATA[
            <p>Hi!</p>
            <p>
            I use the app SAY Hi! and I really love it!
            <br>
            If you also get it, we could use it together.
            </p>
            <p>
            You can download the app from here:
            </p>
            <p>
            iOS:
            <br>
            <a href=\'%1$s\'>%2$s</a>
            <p>
            <p>
            Android:
            <br>
            <a href=\'%3$s\'>%4$s</a>
            </p>
            Bye!
            </p>
            <p>%5$s</p>
        ]]>
    </string>
